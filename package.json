{
  "name": "@flex-development/toggle-pkg-type",
  "description": "Toggle type fields in package.json files",
  "version": "1.1.1",
  "keywords": [
    "cjs",
    "commonjs",
    "esbuild",
    "esm",
    "interop",
    "module",
    "package.json",
    "toggle",
    "type",
    "typescript"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/flex-development/toggle-pkg-type",
  "repository": "https://github.com/flex-development/toggle-pkg-type.git",
  "bugs": "https://github.com/flex-development/toggle-pkg-type/issues",
  "author": {
    "name": "Lexus Drumgold",
    "url": "https://github.com/unicornware"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "bin": "./dist/cli.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "mkbuild",
    "changelog": "node --loader=./loader.mjs ./config/changelog.config",
    "check:ci": "yarn dedupe --check && yarn check:format && yarn check:lint && yarn check:spelling && yarn typecheck && yarn test:cov && NODE_ENV=production yarn pack -o %s-%v.tgz && yarn clean:pack && yarn check:types:build",
    "check:format": "prettier --check .",
    "check:lint": "eslint --exit-on-fatal-error --ext cjs,gql,json,jsonc,md,mjs,mts,ts,yml --max-warnings 0 .",
    "check:spelling": "cspell lint --color --no-progress --relative $@ \"**\"",
    "check:types": "tsc -p tsconfig.typecheck.json",
    "check:types:build": "bash ./scripts/typecheck-build.sh",
    "check:upgrades": "yarn upgrade-interactive",
    "clean:build": "trash ./{dist,*.tgz}",
    "clean:coverage": "trash ./coverage",
    "clean:modules": "trash ./.yarn/{cache,*.gz} ./node_modules",
    "clean:pack": "trash ./*.tgz",
    "clean:test": "yarn clean:coverage && trash ./__tests__/report.json",
    "codecov": "yarn test:cov && yarn test:cov:upload",
    "codecov:validate": "cat .codecov.yml | curl --data-binary @- https://codecov.io/validate",
    "fix:cg": "yarn fix:format && yarn fix:lint",
    "fix:dedupe": "yarn dedupe --strategy=highest",
    "fix:format": "prettier --cache --write .",
    "fix:lint": "yarn check:lint --cache --fix",
    "postinstall": "[ -f ./node_modules/.bin/husky ] && chmod +x .husky/* && husky install || exit 0",
    "postpack": "toggle-scripts +postinstall",
    "postpublish": "toggle-scripts +prepack",
    "prepack": "toggle-scripts -postinstall && yarn build",
    "prepublishOnly": "toggle-scripts -prepack",
    "recommended-bump": "conventional-recommended-bump --preset=conventionalcommits --tag-prefix=$(jq .tagPrefix package.json -r) --verbose",
    "release": "bash ./scripts/release.sh",
    "test": "vitest run",
    "test:cov": "yarn clean:coverage; yarn test --coverage",
    "test:cov:serve": "serve -l 5432 ./coverage/lcov-report",
    "test:cov:upload": "./codecov -t $CODECOV_TOKEN -f ./coverage/lcov.info",
    "test:watch": "vitest",
    "typecheck": "vitest typecheck --run",
    "typecheck:watch": "vitest typecheck"
  },
  "dependencies": {
    "@flex-development/mlly": "1.0.0-alpha.15",
    "@flex-development/pathe": "1.0.3",
    "@flex-development/tutils": "6.0.0-alpha.10"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@flex-development/mkbuild": "1.0.0-alpha.16",
    "@flex-development/pkg-types": "2.0.0",
    "@graphql-eslint/eslint-plugin": "3.16.1",
    "@types/chai": "4.3.4",
    "@types/conventional-changelog": "3.1.1",
    "@types/conventional-changelog-core": "4.2.1",
    "@types/conventional-changelog-writer": "4.0.2",
    "@types/conventional-commits-parser": "3.0.3",
    "@types/conventional-recommended-bump": "6.1.0",
    "@types/dateformat": "5.0.0",
    "@types/eslint": "8.21.1",
    "@types/git-raw-commits": "2.0.1",
    "@types/is-ci": "3.0.0",
    "@types/node": "18.14.0",
    "@types/node-notifier": "8.0.2",
    "@types/prettier": "2.7.2",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vates/toggle-scripts": "1.0.0",
    "@vitest/coverage-c8": "0.28.5",
    "@vitest/ui": "0.29.2",
    "add-stream": "1.0.0",
    "chai": "4.3.7",
    "conventional-changelog": "3.1.25",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "conventional-changelog-core": "4.2.4",
    "conventional-changelog-writer": "5.0.1",
    "conventional-commits-parser": "3.2.4",
    "conventional-recommended-bump": "6.1.0",
    "cross-env": "7.0.3",
    "cspell": "6.28.0",
    "dateformat": "5.0.3",
    "esbuild": "0.17.11",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "40.0.1",
    "eslint-plugin-jsonc": "2.6.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-markdownlint": "0.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-yml": "1.5.0",
    "graphql": "16.6.0",
    "graphql-config": "4.5.0",
    "growl": "1.10.5",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jsonc-eslint-parser": "2.1.0",
    "lint-staged": "13.1.2",
    "memfs": "3.4.13",
    "mri": "1.2.0",
    "node-notifier": "10.0.1",
    "prettier": "2.8.4",
    "prettier-plugin-sh": "0.12.8",
    "sade": "1.8.1",
    "serve": "14.2.0",
    "tempfile": "5.0.0",
    "trash-cli": "5.0.0",
    "ts-dedent": "2.2.0",
    "typescript": "5.0.0-dev.20230222",
    "version-bump-prompt": "6.1.0",
    "vite": "4.1.4",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.28.5",
    "yaml-eslint-parser": "1.1.0"
  },
  "resolutions": {
    "@ardatan/sync-fetch": "larsgw/sync-fetch#head=worker_threads",
    "@flex-development/tutils": "6.0.0-alpha.10"
  },
  "engines": {
    "node": ">=14.17.0",
    "yarn": "4.0.0-rc.39"
  },
  "packageManager": "yarn@4.0.0-rc.39",
  "readme": "README.md",
  "sideEffects": false,
  "tagPrefix": ""
}
